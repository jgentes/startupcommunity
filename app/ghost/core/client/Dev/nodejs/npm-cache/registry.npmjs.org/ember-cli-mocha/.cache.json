y\n\nThis product includes software developed at\nSwitchfly (http://www.switchfly.com).\n\nNOTICE: Only our own original work is licensed under the terms of the Apache\nLicense Version 2.0. The licenses of some libraries might impose different\nredistribution or general licensing terms than those stated in the Apache\nLicense. Users and redistributors are hereby requested to verify these\nconditions and agree upon them.\n","maintainers":[{"name":"dgeb","email":"dan@cerebris.com"},{"name":"rwjblue","email":"me@rwjblue.com"}],"time":{"modified":"2015-12-11T16:11:42.897Z","created":"2014-11-12T15:30:54.187Z","0.1.0":"2014-11-12T15:30:54.187Z","0.1.1":"2014-11-12T15:50:11.613Z","0.1.2":"2014-11-12T17:24:39.749Z","0.1.3":"2014-11-12T17:56:50.984Z","0.1.4":"2014-11-20T01:09:45.697Z","0.2.0":"2014-12-06T20:59:13.750Z","0.2.1":"2014-12-23T04:11:10.948Z","0.3.0":"2015-01-25T18:38:07.169Z","0.3.1":"2015-01-28T16:05:15.736Z","0.4.0":"2015-02-01T17:01:11.493Z","0.4.1":"2015-02-16T13:32:44.329Z","0.4.2":"2015-02-16T14:10:13.321Z","0.5.0":"2015-02-22T20:07:59.600Z","0.6.0":"2015-04-04T22:39:44.888Z","0.6.1":"2015-05-14T00:03:43.012Z","0.6.2":"2015-05-14T11:56:28.964Z","0.7.0":"2015-05-14T11:59:25.881Z","0.8.0":"2015-06-19T17:01:41.397Z","0.9.0":"2015-07-09T16:26:29.684Z","0.9.1":"2015-07-13T21:03:13.415Z","0.9.2":"2015-08-05T17:47:49.451Z","0.9.3":"2015-09-10T12:00:39.465Z","0.9.4":"2015-11-08T19:36:22.599Z","0.9.5":"2015-11-09T14:39:17.851Z","0.9.6":"2015-11-10T14:55:35.115Z","0.9.7":"2015-11-10T20:07:53.355Z","0.9.8":"2015-12-11T16:11:42.897Z"},"homepage":"https://github.com/switchfly/ember-cli-mocha","keywords":["ember-addon","ember","mocha","chai"],"repository":{"type":"git","url":"git+https://github.com/switchfly/ember-cli-mocha.git"},"bugs":{"url":"https://github.com/switchfly/ember-cli-mocha/issues"},"license":"Apache-2.0","readmeFilename":"README.md","users":{"ivanoats":true,"gregone":true,"darrylr":true},"_attachments":{},"_etag":"\"640J55JMAS4RD3SGBJS4PCZG\""}